{
  "name": "hyperkeys",
  "version": "1.3.1",
  "description": "Global shortcuts on steroids",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron build",
    "build:dependencies": "bash scripts/build-dependencies.sh",
    "build": "bash scripts/build.sh",
    "lint": "eslint src",
    "watch": "bash scripts/watch.sh",
    "package:linux": "bash scripts/package-linux.sh",
    "package:windows": "bash scripts/package-windows.sh",
    "serve": "npm run build; electron build & webpack --watch"
  },
  "author": "Xurei Lab",
  "build": {
    "productName": "HyperKeys",
    "appId": "com.xureilab.hyperkeys",
    "asar": false,
    "mac": {
      "category": "Utility"
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "icon": "300x300.png",
      "extraFiles": [
        {
          "from": "natives/linux/libappindicator3.so.1",
          "to": "usr/lib/libappindicator3.so.1"
        }
      ]
    },
    "directories": {
      "app": "build",
      "output": "dist_packages"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "@babel/runtime-corejs3": "^7.22.15",
    "active-win": "9.0.0",
    "asfw": "git+https://github.com/xurei/node-asfw#d5e9aee8c5de7e85470dda22330e821ca8c971f5",
    "core-js": "^3.32.2",
    "debug": "^4.3.4",
    "electron-json-storage": "^4.6.0",
    "hyperkeys-api": "^1.2.0",
    "node-notifier": "^10.0.1",
    "react-is": "^18.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "semver": "^7.5.4",
    "uuid": "^9.0.1",
    "xurei-util": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@electron/rebuild": "^3.3.0",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "brace-expansion": "^2.0.1",
    "css-loader": "^6.8.1",
    "deep-eql": "^4.1.3",
    "electron": "^26.6.10",
    "electron-builder": "24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.49.0",
    "eslint-config-xurei": "^1.1.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-xurei": "^1.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "indent-stream": "^1.0.0",
    "install": "^0.13.0",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "reactstrap": "^9.2.0",
    "redux": "^4.2.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "styled-components": "^6.0.8",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  }
}
