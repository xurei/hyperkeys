{
  "name": "hyperkeys",
  "version": "1.1.9",
  "description": "Global shortcuts on steroids",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron build",
    "build:dependencies": "bash scripts/build-dependencies.sh",
    "build": "bash scripts/build.sh",
    "lint": "eslint src",
    "watch": "bash scripts/watch.sh",
    "package:linux": "bash scripts/package-linux.sh",
    "package:windows": "bash scripts/package-windows.sh",
    "serve": "npm run build; electron build & webpack --watch"
  },
  "author": "Xurei Lab",
  "build": {
    "productName": "HyperKeys",
    "appId": "com.xureilab.hyperkeys",
    "asar": false,
    "mac": {
      "category": "Utility"
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage"
    },
    "directories": {
      "app": "build",
      "output": "dist_packages"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.9.6",
    "@babel/runtime-corejs3": "^7.9.6",
    "active-win": "^6.1.0",
    "asfw": "git+https://github.com/xurei/node-asfw.git#337a3b513e82c5cb12146a4343c2dbb6c2582f07",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "electron-json-storage": "^4.1.8",
    "gkm": "^0.2.0",
    "hyperkeys-api": "^1.1.2",
    "node-notifier": "^8.0.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "semver": "^7.3.2",
    "uuid": "^7.0.3",
    "xurei-util": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "autobind-decorator": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-imports": "^2.0.0",
    "brace-expansion": "^1.1.11",
    "css-loader": "^3.5.3",
    "deep-eql": "^4.0.0",
    "electron": "^7.2.4",
    "electron-builder": "^22.6.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.1",
    "eslint": "^6.3.0",
    "eslint-config-xurei": "^1.1.1",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-xurei": "^1.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "indent-stream": "^1.0.0",
    "install": "^0.13.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.1",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.4",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.0",
